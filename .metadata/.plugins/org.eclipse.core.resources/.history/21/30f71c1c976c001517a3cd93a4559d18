//Project 2


//Loop thru an array
var words = ["Minie", "Mynie", "Moe"];

for (var i =0 ; i <words.length; i++){
	console.log ("Array i value " + words[i]);
}



//Objects

var God = {
	name: "Thor",
	weapon: "Hammer",
	hairColor: "yellow",
	eyeColor: "blue",
};

console.log(God.name);

//Loop for objects

for (var h in God){
	console.log(God[h]);
}
