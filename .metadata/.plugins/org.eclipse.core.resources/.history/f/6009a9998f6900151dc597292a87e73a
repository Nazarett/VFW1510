//Background color     
Ti.UI.setBackgroundColor("#000");

// Main window 
var mainWindow = Ti.UI.createWindow({
	backgroundColor: "#fff"
	
});

var mainView = Ti.UI.createView({
	backgroundColor: "#333",
	borderRadius: 5,
	borderWidth: 1,
	width: 100,
	height: 100
});

mainWindow.add(mainView);
mainWindow.open();
