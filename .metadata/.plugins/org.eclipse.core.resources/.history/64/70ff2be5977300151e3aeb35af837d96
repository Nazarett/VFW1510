//Function
var prevHero = function(){
	index--;
	heroesText.text = superHeroes [index];
	
if (index == 0){
	index = 5;
	heroesText.text = superHeroes [index];
};
   

};

var nextHero = function(){
	index++;
	heroesText.text = superHeroes [index];
	
if (index == 5){
	index = 0;
	heroesText.text = superHeroes [index];
};

	
};



//Event Listener
prevView.addEventListener("click", prevHero);
nextView.addEventListener("click", nextHero);
